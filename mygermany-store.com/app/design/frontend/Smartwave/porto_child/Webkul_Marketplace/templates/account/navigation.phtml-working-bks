<script>
    var isDashBoard = "<?php echo $isDashBoard;?>";
    console.log('isDashBoard '+isDashBoard);
    require(
        [
            'jquery',
            'mage/translate',
            'Magento_Ui/js/modal/modal'
        ],
        function(
            $,
            $t,
            modal
        ) {
            var options = {
                type: 'popup',
                responsive: true,
                innerScroll: true,
                title: "<?php echo __("Question to Admin"); ?>",
                buttons: [
                    {
                        text: $.mage.__('Reset'),
                        class: 'resetbtn',                        
                        id: 'resetbtn',
                        click: function () {
                            $('.subject').val(null);
                            $('.ask').val(null);
                        }
                    },
                    {
                        text: $.mage.__('Submit'),
                        class: 'mis_askbtn',
                        type: 'submit',
                        id: 'mis_askbtn',
                        click: function () {
                            onClickAction();
                        }
                    }
                ]
                
            };

            var attention_pop = {
                type: 'popup',
                responsive: true,
                innerScroll: true,
                title: $.mage.__('Attention')
            };
            var err_pop = {
                type: 'popup',
                responsive: true,
                innerScroll: true,
                title: $.mage.__('Attention')
            };

            var popup = modal(options, $('#popup-modal'));

            var popup_new = modal(attention_pop, $('#mail-send'));
            var popup_err = modal(attention_pop, $('#mail-err'));

            $('#mis_askque').on('click', function(){
                // console.log('mis_askque');                  
                if (isDashBoard == 0) {
                    $('#popup-modal').modal('openModal');                      
                } else {
                    $('#askque').trigger('click');
                }
                

                // $('.modal-footer').css('display', 'none');
            });

            $('.delimiter').parent().remove();
            $(document).ready(function(){
                windowChange();
            });
            $( window ).resize(function() {
                windowChange();
                
            });
            
            function windowChange() {
                var width = $(window).width(); 
                var height = $(window).height(); 
                if (width < 768 ) {
                    jQuery("#account-nav .item").each(function(){
                        var copy = jQuery(this).clone();
                        jQuery(this).addClass('mis_temp_menus');
                        console.log('added');
                        jQuery(this).css('display','none');
                        jQuery('.mis_menu_items').append(copy);
                    });
                 //do something
                } else {
                    jQuery("#account-nav .item").each(function(){
                        jQuery(this).css('display','block');
                        jQuery('.mis_temp_menus').remove();
                    });
                }
            }
        }
    );
</script>
