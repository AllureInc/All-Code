    var tabArray = [];
    require([
            "jquery"
        ], function($){
            $('.mp-push-notification').css('display','none');
            setTimeout(function(){ 
                console.log('mis-cookie-js');
                $('.js-cookie-settings').on('click', function(){
                    $('.modal-popup').each(function(){   
                        if ($(this).hasClass('confirm') && $(this).hasClass('_show')) {
                            $(this).addClass('mis-confirm');                           
                        }
                    });
                }); 
             }, 5000);
    });
