<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceBlock name="head.components">
            <block class="Magento\Framework\View\Element\Js\Components" name="customer_mpamazonconnector_head_components" template="Magento_Customer::js/components.phtml"/>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Webkul\MpAmazonConnector\Block\Account\AmazonConfig" name="mpamazonconnect-account-index" template="account/amazon-config.phtml" cacheable="false">
            </block>
        </referenceContainer>
    </body>
</page>