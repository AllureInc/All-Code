<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../lib/internal/Magento/Framework/Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Webkul_MpAmazonConnector::mp_amazon_menu" title="Marketplace Amazon Connector">
                    <resource id="Webkul_MpAmazonConnector::manage_amazon_account" title="Manage Amazon Account" sortOrder="10" >
                        <resource id="Webkul_MpAmazonConnector::MpAccounts" title="Amazon Accounts" sortOrder="30"/>
                        <resource id="Webkul_MpAmazonConnector::MpProduct" title="Map Products" sortOrder="10"/>
                        <resource id="Webkul_MpAmazonConnector::MpOrder" title="Map Orders" sortOrder="20"/>
                        <resource id="Webkul_MpAmazonConnector::MpProductToAmazon" title="Export To Amazon" sortOrder="30"/>
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Webkul_MpAmazonConnector::config_mpamazonconnect" title="Marketpalce Amazon Configuration" sortOrder="50" >
                            </resource>
                        </resource>
                    </resource>
                </resource>
            </resource>   
        </resources>
    </acl>
</config>