<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../lib/internal/Magento/Framework/Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Webkul_Marketplace::marketplace" title="Marketplace Management">
                    <resource id="Mangoit_RakutenConnector::manage_account" title="Manage Rakuten Account" sortOrder="10" >
                        <resource id="Mangoit_RakutenConnector::Accounts" title="Rakuten Accounts" sortOrder="30"/>
                        <resource id="Mangoit_RakutenConnector::Product" title="Map Products" sortOrder="10"/>
                        <resource id="Mangoit_RakutenConnector::Order" title="Map Orders" sortOrder="20"/>
                        <resource id="Mangoit_RakutenConnector::ProductToRakuten" title="Export To Rakuten" sortOrder="30"/>
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Mangoit_RakutenConnector::config_rakutenconnect" title="Marketpalce Rakuten Configuration" sortOrder="50" >
                            </resource>
                        </resource>
                    </resource>
                </resource>
            </resource>   
        </resources>
    </acl>
</config>