<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <head>
        <css src="Webkul_Marketplace/css/product.css"/>
        <css src="Mangoit_RakutenConnector::css/style.css"/>
    </head>
    <body>
        <referenceBlock name="head.components">
            <block class="Magento\Framework\View\Element\Js\Components" name="customer_rakutenconnector_head_components" template="Magento_Customer::js/components.phtml"/>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Mangoit\RakutenConnector\Block\Product\Unassigned" name="rakutenconnect-product-unassigned" template="product/unassign-list.phtml" cacheable="false">
            </block>
        </referenceContainer>
    </body>
</page>