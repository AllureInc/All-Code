<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="scommerce_gdpr_customers_export_data">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">scommerce_gdpr_customers_export_data</item>
                        <item name="label" xsi:type="string" translate="true">Export Customer Data</item>
                        <item name="url" xsi:type="url" path="scommerce_gdpr/customer/exportCustomerData"/>
                    </item>
                </argument>
            </action>
            <action name="scommerce_gdpr_customers_delete_gdpr_personal_data">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">scommerce_gdpr_customers_delete_gdpr_personal_data</item>
                        <item name="label" xsi:type="string" translate="true">Delete GDPR Personal Data</item>
                        <item name="url" xsi:type="url" path="scommerce_gdpr/customer/deletePersonalData"/>
                        <item name="confirm" xsi:type="array">
                            <item name="title" xsi:type="string" translate="true">Delete GDPR Personal Data</item>
                            <item name="message" xsi:type="string" translate="true">Are you sure you want delete GDPR Personal Data?</item>
                        </item>
                    </item>
                </argument>
            </action>
        </massaction>
    </listingToolbar>
</listing>
