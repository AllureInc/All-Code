<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="adminhtml.newslettrer.subscriber.grid.massaction">
            <arguments>
                <argument name="options" xsi:type="array">
                    <item name="scommerce_gdpr_newsletter_export_customer_data" xsi:type="array">
                        <item name="label" xsi:type="string" translate="true">Export Customer Data</item>
                        <item name="url" xsi:type="string">scommerce_gdpr/newsletter/exportCustomerData</item>
                    </item>
                    <item name="scommerce_gdpr_newsletter_delete_personal_data" xsi:type="array">
                        <item name="label" xsi:type="string" translate="true">Delete GDPR Personal Data</item>
                        <item name="url" xsi:type="string">scommerce_gdpr/newsletter/deletePersonalData</item>
                        <item name="confirm" xsi:type="string">Are you sure you want delete GDPR Personal Data?</item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
    </body>
</page>